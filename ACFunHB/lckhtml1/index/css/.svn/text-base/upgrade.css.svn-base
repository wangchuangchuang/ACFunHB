html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, menu, nav, section, summary, time, mark, audio, video, input { margin: 0; padding: 0; border: none; outline: 0; vertical-align: baseline;}
html, body {overflow-x:hidden; font-family: "华文细黑"; word-break: break-all; word-wrap: break-word;}
dl,dt,dd,ul,li{ list-style: none; margin: 0; padding: 0;}
header,footer,section,article,aside,nav,address,figure,figcaption,menu,details{display:block;}
table{border-collapse:collapse;border-spacing:0;}
caption,th{text-align:left;font-weight:normal;}
html,body,fieldset,img,iframe,abbr{border:0;}
cite,em,var,address,dfn{font-style:normal;}
[hidefocus],summary{outline:0;}
h1,h2,h3,h4,h5,h6,small{font-size:100%; font-weight: normal;}
pre,code,kbd,samp{font-family:inherit;}
a{cursor:pointer; text-decoration:none; outline:none;}
*{outline:none;}
button{ cursor:pointer; border:0 none;}
input[type="submit"], input[type="reset"], input[type="button"], button{-webkit-appearance: none;}
body,textarea,input,button,select,keygen,legend{outline:0;}
body{background-color: #f5f5f9;}
a,img,button,input,textarea{-webkit-tap-highlight-color:rgba(255,255,255,0);}
a{text-decoration:none;
-webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;}

/*主页*/
.upgrade-prompt-content{
    height: 33px;
    line-height: 33px;
    font-size: 1em;
    color: #fc6156;
    background-color: #fff;
}

.bigbox{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  background: url("../img/bg.png");
  background-repeat: repeat;
  z-index:105;
}
.display-block{
	display:block;
}

.upgrade-prompt-content .icon{
    display: inline-block;
    vertical-align: middle;
    line-height: 33px;
    height: 33px;
    width: 25px;
}
.upgrade-prompt-content .horn{
    background: url("../images/index.png") no-repeat right center;
    -webkit-background-size: 11px auto;
    background-size: 11px auto;
}
.input-box{
    position: relative;
    line-height: 33px;
    height: 33px;
    border-top: 1px #d0d0d4 solid;
    border-bottom: 1px #d0d0d4 solid;
    background-color: #fff;
    margin-top: 15px;
}
.input-box label{
    position: absolute;
    line-height: 33px;
    height: 33px;
    left: 15px;
    top: 0;
    font-size: 14px;
    color: #333;
}
.input-box .card-number{
    margin-left: 85px;
}
.input-box .card-number input{
    line-height: 30px;
    height: 30px;
    font-size: 14px;
    color: #333;
    width: 100%;
}
.btn-submit{
    display: block;
    height: 34px;
    line-height: 34px;
    margin: 0 15px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    background-color: #ed1c24;
    color: #fff;
    font-size: 14px;
    text-align: center;
	margin-top: 17px;
    margin-bottom: 10px;
}
.service-agreement{
    font-size: 12px;
    color: #808080;
    margin: 15px 15px 10px 15px;
}
.upgrade-state-content{
    background-color: #fff;
    border-bottom: 1px #cdcdcd solid;
    padding-top: 15px;
    padding-bottom: 10px;
    margin-bottom: 15px;
}
.upgrade-state-content .icon{
    display: block;
    width: 64px;
    height: 64px;
    -webkit-border-radius: 32px;
    -moz-border-radius: 32px;
    border-radius: 32px;
    margin: 0 auto 15px auto;
    background-color: #fb9f00;
    border-bottom: 1px #cdcdcd solid;
}
.upgrade-state-content .pass{
    background: url("../images/index_06.png") no-repeat center;
    -webkit-background-size: 64px auto;
    background-size: 64px auto;
}
.upgrade-state-content b{
    display: block;
    line-height: 22px;
    font-size: 14px;
    color: #fc6156;
    text-align: center;
}
.upgrade-state-content span{
    display: block;
    line-height: 18px;
    font-size: 12px;
    color: #808080;
    text-align: center;
}

.smallbox div{
	color: #5b5b5b;
    font-size: 12px;
}
.btn-submit_two {
    display: block;
    height: 34px;
    line-height: 34px;
    margin: 0 15px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    background-color: #ed1c24;
    color: #fff;
    font-size: 19px;
    text-align: center;
    margin-top: 37px;
    margin-bottom: 10px;
}